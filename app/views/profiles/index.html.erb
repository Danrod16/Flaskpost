<h1>Your Job Decks</h1>
<p><strong>Select</strong> your job hunting
deck and start <strong>swiping</strong>! </p>

<div class="cards">
    <% @profiles.each do |profile| %>
    <div class="card-category" style="background-image: url(https://res.cloudinary.com/therainycloud/image/upload/v1576167047/Untitled_design_12_n4xgg5.png); position: relative;">
      <p style="font-weight: 100; position: absolute;left: 52px; bottom: 59px;"><%= profile.job_title %></p>
    </div>
    <% end  %>
    link_to
    <div class="card-category" style="align-items: center; justify-content: center; display-flex: vertical;">
      <%= cl_image_tag("https://res.cloudinary.com/therainycloud/image/upload/v1576169127/Create_a_new_Job_hunting_deck_mkivjs.png", style: "height: 80px; width: auto; align-items: flex-center;") %>
    </div>
</div>
